(set-option :strings-exp true)
(declare-fun s () (Seq Int))
(assert (not (or (exists ((i Int)) (not (or (> 0 i) 
                                            (< 0 (seq.nth s i)) 
                                            (> i (seq.len (seq.update s 0 (seq.unit 0)))))))
                 (forall ((j Int)) (or (>= 0 j) 
                                       (>= j (seq.len (seq.update s 0 (seq.unit 0)))) 
                                       (< 0 (seq.nth (seq.update s 0 (seq.unit 0)) j))))
             )))
(check-sat)
