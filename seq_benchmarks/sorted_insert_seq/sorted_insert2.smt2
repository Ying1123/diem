(set-option :strings-exp true)
(declare-fun |'| () (Seq Int))
(declare-fun v () Int)
(assert (not (or (not (< (seq.nth (seq.++ |'| (seq.unit 0)) 0) (seq.nth |'| 1)) ) 
                 (exists ((i Int)) (not (or (>= 0 i) (= v (seq.nth |'| i)) (> i (seq.len (seq.++ |'| (seq.unit 0))))))) 
                 (forall ((j Int)) (or (>= 0 j) 
                                       (>= j (seq.len (seq.++ |'| (seq.unit 0)))) 
                                       (< (seq.nth (seq.++ |'| (seq.unit 0)) 0) (seq.nth (seq.++ |'| (seq.unit v)) j)))))))
(check-sat)
